{
  "name": "h4kuna/ares",
  "type": "library",
  "description": "Provides information about subjects by their identification number from the ARES database (in Czech Republic).",
  "homepage": "https://github.com/h4kuna/ares",
  "license": "MIT",
  "authors": [
    {
      "name": "Milan Matějček",
      "email": "milan.matejcek@gmail.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "h4kuna\\Ares\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "h4kuna\\Ares\\Tests\\": "tests/src/"
    }
  },
  "require": {
    "php": ">=7.4",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-simplexml": "*",
    "nette/utils": "^3.0",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-message": "^1.0"
  },
  "require-dev": {
    "guzzlehttp/guzzle": "^7.5",
    "nette/tester": "^2.4",
    "phpstan/phpstan": "^1.8",
    "phpstan/phpstan-strict-rules": "^1.4",
    "tracy/tracy": "^2.9"
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse",
    "tests": "vendor/bin/tester -s -j 4 --colors 1 -s -C tests/src"
  },
  "suggest": {
    "guzzlehttp/guzzle": "As default implementation for PSR standards."
  }
}
